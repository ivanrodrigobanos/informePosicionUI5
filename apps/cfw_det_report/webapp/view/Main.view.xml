<mvc:View controllerName="cfwdetreport.controller.Main"
    xmlns:mvc="sap.ui.core.mvc" displayBlock="true"
    xmlns:core="sap.ui.core"
    xmlns="sap.m"
    xmlns:f="sap.f"
    xmlns:sfb="sap.ui.comp.smartfilterbar"
    xmlns:sft="sap.ui.comp.smarttable"
    xmlns:customData="http://schemas.sap.com/sapui5/extension/sap.ui.core.CustomData/1">
    <f:DynamicPage fitContent="true" id="dynamicPageId" class="sapUiContentPadding">
        <f:title>
            <f:DynamicPageTitle>
                <f:heading>
                    <Title text="{i18n>title}" />
                </f:heading>
            </f:DynamicPageTitle>
        </f:title>
        <f:header>
            <f:DynamicPageHeader>
                <core:Fragment fragmentName="cfwdetreport.fragment.FilterBar" type="XML" />
            </f:DynamicPageHeader>
        </f:header>
        <f:content>
            <core:Fragment fragmentName="cfwdetreport.fragment.DetailTable" type="XML" />
        </f:content>
    </f:DynamicPage>
</mvc:View>